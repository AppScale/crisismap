goog.provide('cm.LayerModel');

goog.require('cm');
goog.require('cm.Html');
goog.require('cm.LatLonBox');
goog.require('goog.Uri');
goog.require('goog.functions');
goog.require('goog.math');
goog.require('goog.object');
